<taps tapsList="{{tapsList}}" bindtapsChange="tapsChange">
  <view class="order-list">
    <view class="order-item" wx:for="{{orderList}}" wx:key="{{index}}">
      <view class="order-number">
        <text>订单编号</text>
        <text>{{ item.order_number }}</text>
      </view>
      <view class="order-number">
        <text>订单价格</text>
        <text>{{ item.order_price }}</text>
      </view>
      <view class="order-number">
        <text>订单日期</text>
        <text>{{ item.formatTime }}</text>
      </view>
    </view>
  </view>
</taps>