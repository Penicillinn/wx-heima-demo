/* components/search/search.wxss */
.zj-search-wrp {
  height: 90rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
}
.zj-search-wrp navigator {
  height: 100%;
  overflow: hidden;
}
.zj-search-wrp .zj-search {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  background-color: #fff;
  color: #666;
}
